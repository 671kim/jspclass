<%

int year = 3;
String msg1 = "";

switch (year){
case 1: msg1 = "freshman";
		break;
case 2: msg1 = "sophomore";
		break;
case 3: msg1 = "junior";
		break;
case 4: msg1 = "senior";
		break;

default : msg1 = "오류";
}

%>

<%=msg1 %>










